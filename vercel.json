{
  "version": 2,
  "env": {
    "UPLOAD_ASSETS_DIRECTORY": "UPLOAD_ASSETS_DIRECTORY",
    "MAX_FILE_SIZE": "MAX_FILE_SIZE",
    "MONGO_DB_PASSWORD": "MONGO_DB_PASSWORD",
    "MONGO_DB_USER": "MONGO_DB_USER",
    "MONGO_DB_DATABASE_DEV": "MONGO_DB_DATABASE_DEV",
    "MONGO_DB_DATABASE_PROD": "MONGO_DB_DATABASE_PROD",
    "SECRET_TOKEN_KEY": "SECRET_TOKEN_KEY"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  ]
}
